{
  "name": "vendor-machine",
  "version": "1.0.0",
  "description": "A vending machine software.",
  "main": "./server/index.js",
  "directories": {
    "test": "tests"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "client/*"
    ],
    "ignore": []
  },
  "scripts": {
    "build": "rm -rf dist logs && npm run build-prod && npm run copy-folders && webpack --config webpack.prod.config.js && cp -R ./uploads dist/dist",
    "build-dev": "rm -rf dist logs && webpack --config webpack.dev.config.js",
    "build-common": "node_modules/.bin/babel -d ./dist/common ./common",
    "build-prod": "npm run build-server && npm run build-common",
    "build-server": "node_modules/.bin/babel -d ./dist/server ./server",
    "copy-certs": "cp -R ./server/certs dist/server",
    "copy-folders": "npm run copy-certs && npm run copy-json-data",
    "copy-json-data": "mkdir dist/static && cp -R ./static/data dist/static",
    "copy-upload": "cp -R ./uploads dist/uploads",
    "production": "NODE_ENV=production node dist/server/index.js",
    "start": "npm run build-dev && npm run copy-upload && NODE_ENV=development nodemon --exec node_modules/.bin/babel-node ./server/index.js",
    "test": "NODE_ENV=test mocha -r tests/helper.js tests/test.spec.js"
  },
  "keywords": [
    "vendor",
    "machine",
    "application",
    "software",
    "node",
    "express",
    "react",
    "react-router",
    "redux",
    "webpack"
  ],
  "author": "@ramkrishna",
  "license": "ISC",
  "dependencies": {
    "@babel/node": "^7.10.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "axios": "^0.20.0",
    "classnames": "^2.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "formidable": "^1.2.2",
    "helmet": "^4.1.1",
    "lodash": "^4.17.20",
    "log4js": "^6.3.0",
    "mongoose": "^5.10.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-poppop": "^1.5.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^7.0.4",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.2",
    "spdy": "^4.0.2",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-arrow-functions": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.13.16",
    "@hot-loader/react-dom": "^16.13.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.11.1",
    "css-loader": "^4.2.1",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.3.0",
